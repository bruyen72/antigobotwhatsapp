{
  "version": 2,
  "builds": [
    {
      "src": "start.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "start.js"
    }
  ],
  "functions": {
    "start.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/**/*": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "env": {
    "NODE_ENV": "production",
    "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "1"
  }
}